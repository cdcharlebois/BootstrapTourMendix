<?xml version="1.0" encoding="utf-8" ?>
<widget id="Intro.widget.Intro" needsEntityContext="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>Intro</name>
    <description>The description of this widget.</description>

    <icon></icon>





    <properties>
        <property key="beforeMf" type="microflow" required="false">
            <caption>Auto Run</caption>
            <category>Behavior</category>
            <description>If the microflow returns true, the walkthrough will run, leave empty to run each pageload. By default, the walkthrough will only run once. It will always run when the user clicks the button. Implement a microflow here to override the default "once" behavior</description>
            <returnType type="Boolean"></returnType>
        </property>
        <property key="afterMf" type="microflow" required="false">
            <caption>Callback</caption>
            <category>Behavior</category>
            <description>This Microflow will run when the user completes the walkthrough.</description>
            <returnType type="Void"></returnType>
        </property>
        <property key="buttonText" type="string" required="false" defaultValue="">
            <caption>Button Text</caption>
            <category>Appearance</category>
            <description>The text on the button</description>
        </property>
        <property key="errorMessage" type="string" required="true" defaultValue="">
            <caption>Error Message</caption>
            <category>Appearance</category>
            <description>Message to display if the Walkthrough Initialization fails.</description>
        </property>
        <property key="buttonBootstrap" type="enumeration" defaultValue="default">
            <caption>Button Class</caption>
            <category>Appearance</category>
            <description>Select a standard styling for the button</description>
            <enumerationValues>
                <enumerationValue key="default">Default</enumerationValue>
                <enumerationValue key="primary">Primary</enumerationValue>
                <enumerationValue key="info">Info</enumerationValue>
                <enumerationValue key="warning">Warning</enumerationValue>
                <enumerationValue key="danger">Danger</enumerationValue>
                <enumerationValue key="inverse">Inverse</enumerationValue>
            </enumerationValues>
        </property>
        <property key="buttonExtraClass" type="string" required="false" defaultValue="">
            <caption>Extra Button Class</caption>
            <category>Appearance</category>
            <description>Extra Classes to Apply to the Button</description>
        </property>
        <property key="steps" type="object" required="false" isList="true">
            <caption>Intro steps</caption>
            <category>Walkthrough</category>
            <description>Each item in this list will render as a 'step' in the walkthrough.</description>
            <properties>
                <property key="selector" type="string" required="false">
                    <caption>Selector</caption>
                    <category>Main</category>
                    <description>Selector for the element. (If empty, the walkthrough step will appear in the middle of the screen)</description>
                </property>
                <property key="isMendixName" type="boolean" required="true" defaultValue="true">
                    <caption>Selector is Name</caption>
                    <category>Main</category>
                    <description>Choose 'Yes' is you're using the Mendix component name as the selector</description>
                </property>
                <property key="stepTitle" type="string" required="false" defaultValue="">
                    <caption>Title</caption>
                    <category>Main</category>
                    <description>The text that will appear as the title of the popover step</description>
                </property>
                <property key="intro" type="string" required="false">
                    <caption>Text</caption>
                    <category>Main</category>
                    <description>The text that will show up in the walkthrough step. HTML tags are welcome.</description>
                </property>
                <property key="position" type="enumeration" defaultValue="top">
                    <caption>Popover Position</caption>
                    <category>Main</category>
                    <description>The position (relative to `Element`) for the walkthrough step to appear</description>
                    <enumerationValues>
                        <enumerationValue key="top">Top</enumerationValue>
                        <enumerationValue key="right">Right</enumerationValue>
                        <enumerationValue key="bottom">Bottom</enumerationValue>
                        <enumerationValue key="left">Left</enumerationValue>
                    </enumerationValues>
                </property>
                 <property key="backdrop" type="boolean" required="true" defaultValue="true">
                     <caption>Show Backdrop?</caption>
                     <category>Main</category>
                     <description>If enabled, the rest of the page will have a dark filter. Useful to highlighting main content areas.</description>
                 </property>
                  <property key="clickToContinue" type="boolean" required="true" defaultValue="false">
                      <caption>Click to Continue</caption>
                      <category>Main</category>
                      <description>If enabled, clicking on `Element` will progress to walkthrough. Useful when highlighting buttons.</description>
                  </property>
            </properties>
        </property>
    </properties>
</widget>
